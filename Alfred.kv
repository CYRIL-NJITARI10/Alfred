            keep_ratio: True
        Label:
            id : first_itrm
            text: root.first
            pos_hint: {'center_x': 0.2, 'center_y': 1}
        Label:
            id : second_itrm
            text: root.second
            pos_hint: {'center_x': 0.5, 'center_y': 1}
        Label:
            id : third_itrm
            text: root.third
            pos_hint: {'center_x': 0.3, 'center_y': 1}
        Label:
            text: root.proposition
            color : 1,1,1,1
            font_name: 'Comic'
            #padding: (self.textinput.width - width of line) / 2, 20, 0, 0
            size_hint: 0.8, 0.2
            #font_size: '1.4833cm'
            pos_hint: {'center_x': 0.5, 'center_y': 0.5}
            font_size: (root.width**2 + root.height**2) / 14**4

        Button:
            pos_hint: {'center_x': 0.2, 'center_y': 0.2}
            size_hint:0.2, 0.1
            background_color: 0,0,0,0
            on_press:
                root.retrait_point_majeure()
                root.new_proposition()
            Image:
                source: "images/annuler.PNG"
                size: 50,50
                center_x: self.parent.center_x
                center_y: self.parent.center_y

        Button:
            pos_hint: {'center_x': 0.53, 'center_y': 0.2}
            size_hint:0.2, 0.1
            background_color: 0,0,0,0
            text: "Neutre"
            on_press:root.new_proposition()
            Image:
                source: "images/douteux.PNG"
                size: 75,75
                center_x: self.parent.center_x
                center_y: self.parent.center_y

        Button:
            pos_hint: {'center_x': 0.8, 'center_y': 0.2}
            size_hint:0.2,0.1
            background_color: 0,0,0,0
            on_press:
                root.ajout_point_majeure()
                root.new_proposition()
            Image:
                source: "images/amour-vert.PNG"
                size: 50, 50
                center_x: self.parent.center_x
                center_y: self.parent.center_y


        Button:
            pos_hint: {'center_x': 0.5, 'center_y': 0.1}
            size_hint:0.2,0.1
            text: "Retour"
            background_color: 0,0,0,0
            on_release:
                app.root.current = "Accueil"
                root.manager.transition.direction = "right"
            Image:
                source: "images/retour.PNG"
                size: 120,120
                center_x: self.parent.center_x
                center_y: self.parent.center_y



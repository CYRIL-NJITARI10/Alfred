<MajeureWindow>:
    name: "Majeure"

    first_itrm:first_itrm
    second_itrm:second_itrm
    third_itrm:third_itrm

    canvas.before:
        Color:
            rgba: ( 1,1,1,1)
        Rectangle :
            pos: self.pos
            size: self.size
    FloatLayout:
        orientation:"vertical"
        Image:
            source:'images/img.png'
            size_hint: 0.2, 0.15
            pos_hint: {'center_x': 0.88, 'center_y': 0.93}
            allow_stretch: True
            keep_ratio: True
        Image:
            source:'images/ALFREDBON.png'
            size_hint: 0.9, 0.5
            pos_hint: {'center_x': 0.5, 'center_y': 0.82}
            allow_stretch: True
            keep_ratio: True
        Image:
            source:'images/arrplan.png'
            size_hint: 0.9, 0.5
            pos_hint: {'center_x': 0.5, 'center_y': 0.45}


#fenêtre de login
<LoginWindow>:
    name: "login" # le name est comme une clé qui permet de faire reférence à la fenetre login

    intermediaire : itmd
    current_email:current_email
    email: email
    password: password

    canvas.before:
        Color:
            rgba: ( 1,1,1,1)
        Rectangle :
            pos: self.pos
            size: self.size
    FloatLayout:
        orientation:"vertical"
        Image:
            source:'images/img.png'
            size_hint: 0.2, 0.15
            pos_hint: {'center_x': 0.88, 'center_y': 0.93}
            allow_stretch: True
            keep_ratio: True
        Image:
            source:'images/ALFREDBON.png'
            size_hint: 0.9, 0.5
            pos_hint: {'center_x': 0.5, 'center_y': 0.82}
            allow_stretch: True
            keep_ratio: True
        Label:
            id : itmd
            pos_hint: {'center_x': 0.2, 'center_y': 0.5}
        Label:
            id : current_email
            pos_hint: {'center_x': 0, 'center_y': 1}
        TextInput:
            id: email
            font_size: (root.width**2 + root.height**2) / 13**4
            multiline: False
            hint_text:"E-mail"
            pos_hint: {'center_x': 0.5, 'center_y': 0.5}
            size_hint: 0.6, 0.09

        TextInput:
            id: password
            font_size: (root.width**2 + root.height**2) / 13**4
            multiline: False
            hint_text:"Password"
            password: True
            #pos_hint: {"x": 0.35, "top":0.7}
            pos_hint: {'center_x': 0.5, 'center_y': 0.3}
            size_hint: 0.6, 0.09

        Button:
            pos_hint:{"x":0.55,"y":0.05}
            size_hint: 0.35, 0.1
            font_size: (root.width**2 + root.height**2) / 13**4
            text: "Se connecter"
            background_color: 0,0,0,0
            on_release:
                root.manager.transition.direction = "up"
                root.loginBtn();
                #root.manager.get_screen('Majeure').update_text(root.intermediaire.text)
            Image:
                source: "images/seco.PNG"
                size: 170,170
                center_x: self.parent.center_x
                center_y: self.parent.center_y

        Button:
            pos_hint:{"x":0.15,"y":0.05}
            size_hint: 0.35, 0.1
            font_size: (root.width**2 + root.height**2) / 13**4
            text: "Créer un compte"
            background_color: 0,0,0,0
            on_release:
                root.manager.transition.direction = "right"
                root.createBtn()
            Image:
                source: "images/cre.PNG"
                size: 170,170
                center_x: self.parent.center_x
                center_y: self.parent.center_y


#fenêtre de création de compte

<CreateAccountWindow>:
    name: "create" # identifiant de la fenetre

    namee: namee
    email: email
    password: passw
    canvas.before:
        Color:
            rgba: ( 1,1,1,1)
        Rectangle :
            pos: self.pos
            size: self.size
    FloatLayout:
        orientation:"vertical"
        Image:
            source:'images/img.png'
            size_hint: 0.2, 0.15
            pos_hint: {'center_x': 0.88, 'center_y': 0.93}
            allow_stretch: True
            keep_ratio: True
        Image:
            source:'images/ALFREDBON.png'
            size_hint: 0.9, 0.5
            pos_hint: {'center_x': 0.5, 'center_y': 0.82}
            allow_stretch: True
            keep_ratio: True

        Label:
            text: "Créer un compte"
            size_hint: 0.8, 0.2
            pos_hint: {'center_x': 0.15, 'center_y': 0.58}
            font_size: (root.width**2 + root.height**2) / 14**4


        TextInput:
            pos_hint: {"x":0.5, "top":0.8}
            size_hint: 0.6, 0.09
            id: namee
            hint_text:"User name"
            pos_hint: {'center_x': 0.5, 'center_y': 0.5}
            multiline: False
            font_size: (root.width**2 + root.height**2) / 14**4

        TextInput:
            id: email
            size_hint: 0.6, 0.09
            hint_text:"E-mail"
            pos_hint: {'center_x': 0.5, 'center_y': 0.4}
            multiline: False
            font_size: (root.width**2 + root.height**2) / 14**4


        TextInput:
            id: passw
            pos_hint: {'center_x': 0.5, 'center_y': 0.3}
            size_hint: 0.6, 0.09
            hint_text:"password"
            multiline: False
            password: True
            font_size: (root.width**2 + root.height**2) / 14**4

        Button:
            pos_hint:{"x":0.15,"y":0.05}
            size_hint: 0.35, 0.1
            font_size: (root.width**2 + root.height**2) / 13**4
            background_color: 0,0,0,0
            text: "Se connecter"
            on_release:
                app.root.current = "login"
                root.manager.transition.direction = "left"
            Image:
                source: "images/seco.PNG"
                size: 170,170
                center_x: self.parent.center_x
                center_y: self.parent.center_y

        Button:
            pos_hint:{"x":0.55,"y":0.05}
            size_hint: 0.35, 0.1
            text: "S'inscrire"
            background_color: 0,0,0,0
            font_size: (root.width**2 + root.height**2) / 13**4
            on_release:
                root.manager.transition.direction = "left"
                root.submit()
            Image:
                source: "images/sins.PNG"
                size: 170,170
                center_x: self.parent.center_x
                center_y: self.parent.center_y


